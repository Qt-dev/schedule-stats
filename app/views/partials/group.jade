mixin group(group, groupTitle)
  div.group
    h2= groupTitle

    ul
      each subset, subsetTitle in group
        li.subset
          h3.subset-title= subsetTitle
          ul
            each stat, statTitle in subset
              li.stat
                h4.stat-title= statTitle
                - if(typeof(stat) === 'object')
                  ul
                    each subStat, subStatKey in stat
                      li.substat
                        h5.substat-title=subStatKey
                        p= subStat
                - else
                  p= stat

